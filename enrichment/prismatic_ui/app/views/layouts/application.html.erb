    <body>
      <section id="container" class="">
        <!--header start-->
        <%= render "header" %>
        <!--header end-->
        <!--sidebar start-->
        <%= render "sidebar" %>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
          <section class="wrapper site-min-height">
            <%= render :partial=>"flash_message" %>
            <!-- page start-->
            <%= yield %>
            <!-- page end-->
          </section>
        </section>
