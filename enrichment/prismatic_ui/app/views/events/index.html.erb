<div class="row">
  <div class="col-lg-12">
    <section class="panel">
      <header class="panel-heading">
        Events
        <span class="pull-right">
          <%= link_to '+ New Event', new_event_path ,:class=>"btn btn-primary"%>
        </span>  
      </header> 
      <div class="panel-body">
        <div class="adv-table">
          <table  class="display table table-bordered table-striped">
            <thead>

              <tr>
                <th>Event Name</th>
                <th>Object</th>
                <th>Object class</th>
                <th>User</th>
                <th>Session</th>
                
                <th>Event time</th>
                <th>Action class</th>
                <th>Action type</th>
                
              </tr>
            </thead>
            <tbody>
             <% @events.each do |event| %>
             <tr>
              <td><%=link_to event.name,event_path(event) %></td>
              <td><%= event.object_id %></td>
              <td><%= event.object_class %></td>
              <td><%= event.user_id %></td>
              <td><%= event.session_id %></td>
              <td><%= event.event_time %></td>
              <td><%= event.action_class %></td>
              <td><%= event.action_type %></td>

            </tr>
            <% end %>

 
          </tbody>
        </table>  
          <hr />
          <%= page_entries_info @events %> 
          <div class="pull-right pagination-wrapper">   
            <%= paginate @events %>
          </div>
      </div>
    </div>  
  </section> 
</div>
</div>    


